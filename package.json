{
  "name": "vizceral",
  "version": "2.1.2",
  "description": "Intuitive animated traffic graph using webgl. If a graph of nodes and edges with data about traffic volume is provided, it will render a traffic graph animating the connection volume between nodes.",
  "main": "dist/vizceral.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:Netflix/vizceral.git"
  },
  "scripts": {
    "dev": "npm run watch",
    "doc": "documentation build ./src/vizceral.js --shallow -f md > USAGE.md",
    "lint": "eslint src",
    "build": "npm run doc && npm run build:js",
    "build:js": "webpack --optimize-occurrence-order",
    "watch": "webpack --watch -d",
    "release": "release",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "css-loader": "^0.23.1",
    "documentation": "^4.0.0-beta2",
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^4.0.0",
    "eslint-plugin-import": "^1.10.2",
    "file-loader": "^0.9.0",
    "raw-loader": "^0.5.1",
    "release-script": "^1.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "worker-loader": "^0.7.0"
  },
  "dependencies": {
    "chroma-js": "^1.1.1",
    "hammerjs": "^2.0.8",
    "knuth-shuffle": "^1.0.1",
    "lodash": "^4.12.0",
    "numeral": "^1.5.3",
    "three": "^0.78",
    "tween.js": "^16.3.4",
    "snyk": "^1.316.1"
  },
  "keywords": [
    "webgl",
    "traffic",
    "graph",
    "particles",
    "flow",
    "intuition",
    "visceral"
  ],
  "author": "Justin Reynolds <jreynolds@netflix.com>",
  "license": "Apache-2.0",
  "snyk": true
}
